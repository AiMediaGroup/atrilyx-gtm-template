___SANDBOXED_JS_FOR_WEB_TEMPLATE___

const log = require('logToConsole');
const createQueue = require('createQueue');
const injectScript = require('injectScript');

// onSuccess, onFailure for launching the tracking code
const onSuccess = () => {
  log('Atrilyx loaded successfully.');
  data.gtmOnSuccess();
};

const onFailure = () => {
  log('Atrilyx has failed to load.');
  data.gtmOnFailure();
};

let _aianalytics;
let ai_jsTracker;

//Must be an AiConnect proxy - ACP does not support this.
let u = data.proxyName; 

// Initialize tracker objects, Atrilyx queue
_aianalytics = createQueue('_paq');
ai_jsTracker = "https://" + u + "/atrilyx.js";

function trackStandard(d){
	if (d.setExtDomains != false) {
      if(d.setExtDomains == 'domains'){
        _aianalytics(['AiMediaGroupExtension::setExtDomains', d.extDomainArray]);
      }else{
        _aianalytics(['AiMediaGroupExtension::setExtDomains', d.setExtDomains]);
      }
	}
	
	// Analytics domains
	_aianalytics(['setDomains', d.analyticsDomains]);

	// Set custom document title
	if (d.setDocumentTitle == true) {
		_aianalytics(["setDocumentTitle", d.documentTitle]);
	}
  
	// Link tracking
	if (d.enableLinkTracking == true) {
		_aianalytics(["enableLinkTracking"]);
	}

	// Cross-domain tracking
	if (d.enableCrossDomainLinking == true) {
		_aianalytics(['enableCrossDomainLinking']);
	}

	// Set cookie domain
	if (d.setCookieDomain == true) {
		_aianalytics(['setCookieDomain', d.cookieDomain]);
	}

	// Setting the User ID
	if (d.setUserID == true) {
		let userId = d.userID;
		_aianalytics(['setUserId', userId]);
	}

	// Enable tracking JS errors as Custom Events for Analytics
	if (d.enableJSErrorTracking == true) {
		_aianalytics(['enableJSErrorTracking']);
	}
  
	// If the option "don't track page views" is not selected, they will be tracked normally
	if (d.dontTrackPageViews != true) {
		_aianalytics(["trackPageView"]);
	}
}

function trackEcomm(d) {
	if(d.addOtherItems != null) {
		_aianalytics(['AiMediaGroupExtension::addOtherItems', d.addOtherItems]);
      log(d.addOtherItems);
    }
  	if(d.ecommerceLabel != null) {
      _aianalytics(['AiMediaGroupExtension::addEcommerceLabel', d.ecommerceLabel]);
      log(d.ecommerceLabel);
    }
    if(d.addLeadEvent != null) {
      _aianalytics(['trackEvent', d.addLeadCT, d.addLeadLT, d.addLeadEvent]);
    }
	var id = d.orderID;
	var amt = d.orderAmount;
	var info = d.productsPurchased;
	for(var i=0; i<info.length; i++) {
		_aianalytics(['addEcommerceItem',
		   info[i].id || info[i].sku || info[i].item_id,
		   info[i].name || info[i].item_name || "",
		   info[i].category || info[i].item_category || "",
		   info[i].price || "",
		   info[i].quantity || ""
		]);
	}
	_aianalytics(['trackEcommerceOrder',
		id,
		amt,
		false
	]);
}

function trackGoal(d){
	_aianalytics(['trackGoal', d.goalID, d.goalValue || null, d.goalOID]);
	if(d.nolegacyLead == false) {
		_aianalytics(['trackEvent', d.contactType, d.leadType, d.leadName]);
